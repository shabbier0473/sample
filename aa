pipeline{
    agent any 
    parameters{
        gitParameter branchFilter: 'origin/(.*)', defaultValue: 'origin/master', name: 'BRANCH', type: 'PT_BRANCH'
        gitParameter name: 'TAG',type: 'PT_TAG', selectedValue: 'NONE'
    }
    stages{
        stage('compile'){
            tools { maven "MAVEN_HOME" }
            steps{
                sh 'mvn compile'
            }
            
        }
    }
}    
